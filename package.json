{
  "name": "rss-discord-transfer",
  "version": "1.0.0",
  "description": "AWS Lambda function to transfer RSS feed items to Discord via webhook",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
    "package": "npm run build && npm run package:zip",
    "package:zip": "cd dist && zip -r ../rss-discord-transfer.zip ."
  },
  "keywords": [
    "aws-lambda",
    "rss",
    "discord",
    "typescript",
    "webhook"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@aws-sdk/util-stream-node": "^3.370.0",
    "@types/aws-lambda": "^8.10.119",
    "@types/jest": "^29.5.3",
    "@types/nock": "^10.0.3",
    "@types/node": "^20.4.5",
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.45.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.6.1",
    "nock": "^14.0.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.377.0",
    "aws-sdk-client-mock": "^4.1.0",
    "fast-xml-parser": "^4.2.7"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
